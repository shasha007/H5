/*全校活动列表*/
.h-scheve {
    position: fixed;
    top:0;
    display: flex;
    width: 100%;
    height: 40px;
    max-width: 640px;
    margin: 0 auto;
    /*margin-bottom: -10px;*/
    background-color: #F2F1ED;
    text-align: center;
    justify-content: center;
    align-items: center;
    z-index: 999;
}

.h-scheve div {
    /*color: #EEEDEB;*/
    flex: 1;
    border-right: 1px solid #E1E0DC;
}

.h-scheve-show1 {
    position: fixed;
    top:40px;
    width: 100%;
    max-width: 640px;
    /*height: 0%;*/
    /*display: none;*/
    margin: auto;
    background-color: #fff;
    z-index: 999;
}
.h-show1-show{
    display: inherit !important;
    height: auto!important;
    animation:fadeIn 0.5s !important;
    -webkit-animation:fadeIn 0.5s !important;
}
.h-scheve-show1 header {
    height: 40px;
    line-height: 40px;
    text-align: center;
    color:#ea5504;
    border-bottom: 1px solid #efefef;
}
.h-show-1 ul{
    margin-top: 10px;
}
.h-show-1 li {
    float: left;
    width: 22%;
    margin-right: 4%;
    padding: 5px 0;
    font-size: 14px;
}
.h-show-1 li > img{
    border-radius: 100%;
}
.h-show-1 li > p {
    text-align: center;

}
.h-show-1 li:nth-child(4n) {
    margin-right: 0;
}


.h-show-2 > li{
    width: 100%;
    height: 40px;
    padding: 0 15px;
    line-height: 40px;
    border-bottom: 1px solid #EFEFEF;
}
.h-show-3 > li{
    width: 50%;
    height: 40px;
    float: left;
    padding: 0 15px;
    line-height: 40px;
    border-bottom: 1px solid #EFEFEF;
    border-right: 1px solid #EFEFEF;
}

@-webkit-keyframes fadeIn {
    0% {
        opacity: 0; /*初始状态 透明度为0*/
    }
    50% {
        opacity: 0; /*中间状态 透明度为0*/
    }
    100% {
        opacity: 1; /*结尾状态 透明度为1*/
    }
}
