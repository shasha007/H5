}

.add-friend {
    width: 70%;
    height: 25px;
    margin: 0 15%;
    line-height: 25px;
    font-size: 12px;
    text-align: center;
    color: #ea5504;
    border: 1px solid #ea5504;
    border-radius: 5px;
}

.dis-person {
    padding-bottom: 5px;
}

.dis-person-con {
    width: 22%;
